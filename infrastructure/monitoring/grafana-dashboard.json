{
  "dashboard": {
    "id": null,
    "uid": "wolfhunt-trading-dashboard",
    "title": "WolfHunt Trading Bot Dashboard",
    "description": "Comprehensive monitoring dashboard for the dYdX trading bot",
    "tags": ["trading", "dydx", "wolfhunt", "monitoring"],
    "timezone": "UTC",
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "user_id",
          "type": "query",
          "query": "SELECT DISTINCT user_id FROM trades ORDER BY user_id",
          "datasource": "PostgreSQL",
          "refresh": 1,
          "includeAll": false,
          "allValue": "",
          "current": {
            "selected": true,
            "text": "2",
            "value": "2"
          }
        },
        {
          "name": "symbol",
          "type": "query", 
          "query": "SELECT DISTINCT symbol FROM trades ORDER BY symbol",
          "datasource": "PostgreSQL",
          "refresh": 1,
          "includeAll": true,
          "allValue": "*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "up{job=\"trading-bot\"}",
            "legendFormat": "System Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "Down"}}, "type": "value"},
              {"options": {"1": {"text": "Up"}}, "type": "value"}
            ],
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Portfolio Value",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT NOW() as time, SUM(notional_value) + SUM(COALESCE(unrealized_pnl, 0)) as value FROM positions WHERE user_id = $user_id AND is_open = true UNION SELECT NOW() as time, 10000 as value"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {"mode": "value"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 9500},
                {"color": "green", "value": 10000}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Daily P&L",
        "type": "stat", 
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT NOW() as time, SUM(COALESCE(realized_pnl, 0)) as value FROM trades WHERE user_id = $user_id AND DATE(timestamp) = CURRENT_DATE"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {"mode": "value"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": -200},
                {"color": "yellow", "value": -50},
                {"color": "green", "value": 0},
                {"color": "light-green", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Active Positions",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT NOW() as time, COUNT(*) as value FROM positions WHERE user_id = $user_id AND is_open = true"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "value"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 3},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Trading Activity (Last 24h)",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT timestamp as time, COUNT(*) OVER (PARTITION BY DATE_TRUNC('hour', timestamp)) as \"Trades per Hour\" FROM trades WHERE user_id = $user_id AND timestamp > NOW() - INTERVAL '24 hours' ORDER BY timestamp"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "fillOpacity": 80,
              "gradientMode": "none"
            },
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Position P&L Distribution",
        "type": "piechart",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT symbol as metric, SUM(COALESCE(unrealized_pnl, 0)) as value FROM positions WHERE user_id = $user_id AND is_open = true GROUP BY symbol"
          }
        ],
        "options": {
          "pieType": "donut",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        }
      },
      {
        "id": 7,
        "title": "Trade Performance by Symbol",
        "type": "table",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 17},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT symbol as \"Symbol\", COUNT(*) as \"Total Trades\", SUM(CASE WHEN realized_pnl > 0 THEN 1 ELSE 0 END) as \"Winning Trades\", ROUND((SUM(CASE WHEN realized_pnl > 0 THEN 1 ELSE 0 END) * 100.0 / COUNT(*))::numeric, 2) as \"Win Rate %\", ROUND(SUM(COALESCE(realized_pnl, 0))::numeric, 2) as \"Total P&L\", ROUND(AVG(COALESCE(realized_pnl, 0))::numeric, 2) as \"Avg P&L\", ROUND(SUM(notional_value)::numeric, 2) as \"Total Volume\" FROM trades WHERE user_id = $user_id AND status = 'FILLED' AND symbol LIKE '%$symbol%' GROUP BY symbol ORDER BY \"Total P&L\" DESC"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": true, "displayName": "Total P&L"}]
        }
      },
      {
        "id": 8,
        "title": "API Response Times",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "histogram_quantile(0.95, rate(api_call_latency_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "datasource": "Prometheus", 
            "expr": "histogram_quantile(0.50, rate(api_call_latency_bucket[5m]))",
            "legendFormat": "50th percentile (median)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        }
      },
      {
        "id": 9,
        "title": "System Resources",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU Usage %"
          },
          {
            "datasource": "Prometheus",
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage MB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Recent Trades",
        "type": "table",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 34},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT timestamp as \"Time\", symbol as \"Symbol\", side as \"Side\", size as \"Size\", price as \"Price\", ROUND(notional_value::numeric, 2) as \"Value\", ROUND(COALESCE(realized_pnl, 0)::numeric, 2) as \"P&L\", status as \"Status\", strategy_name as \"Strategy\" FROM trades WHERE user_id = $user_id ORDER BY timestamp DESC LIMIT 20"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": true, "displayName": "Time"}]
        },
        "transformations": [
          {
            "id": "convertFieldType",
            "options": {
              "conversions": [
                {"destinationType": "time", "targetField": "Time"}
              ]
            }
          }
        ]
      },
      {
        "id": 11,
        "title": "Strategy Signals",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 44},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT timestamp as time, COUNT(*) FILTER (WHERE signal_type = 'BUY') as \"Buy Signals\", COUNT(*) FILTER (WHERE signal_type = 'SELL') as \"Sell Signals\", COUNT(*) FILTER (WHERE signal_type = 'HOLD') as \"Hold Signals\" FROM strategy_signals WHERE user_id = $user_id AND timestamp > NOW() - INTERVAL '24 hours' GROUP BY DATE_TRUNC('hour', timestamp), timestamp ORDER BY timestamp"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "bars",
              "barAlignment": 0
            },
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 12,
        "title": "Alert Summary",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 44},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT 'Critical' as metric, COUNT(*) as value FROM alerts WHERE severity = 'CRITICAL' AND timestamp > NOW() - INTERVAL '24 hours' UNION SELECT 'Warning' as metric, COUNT(*) as value FROM alerts WHERE severity = 'WARNING' AND timestamp > NOW() - INTERVAL '24 hours' UNION SELECT 'Info' as metric, COUNT(*) as value FROM alerts WHERE severity = 'INFO' AND timestamp > NOW() - INTERVAL '24 hours'"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "value"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      }
    ],
    "style": "dark",
    "schemaVersion": 27,
    "version": 1
  },
  "overwrite": true
}